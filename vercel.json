{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/movies",
      "dest": "/api/movies/index.js"
    },
    {
      "src": "/series",
      "dest": "/api/series/index.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}
