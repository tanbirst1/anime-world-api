{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/movies/(.*)",
      "dest": "/api/movies/$1.js"
    },
    {
      "src": "/series/(.*)",
      "dest": "/api/series/$1.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}
